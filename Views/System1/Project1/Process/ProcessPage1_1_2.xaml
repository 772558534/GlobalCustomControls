<UserControl
    x:Class="GlobalCustomControls.ProcessPage1_1_2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:GlobalCustomControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	d:DataContext="{d:DesignInstance Type=local:ProcessPage1_1_2ViewModel}"
	d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="3*" />
        </Grid.RowDefinitions>
        <StackPanel
            Grid.Row="0"
            HorizontalAlignment="Center"
            Orientation="Horizontal">
            <Button
                Width="80"
                Height="30"
                Margin="20"
				Command="{Binding LogCommand}"
                Content="流程1-1-2" />
            <Button
                Width="80"
                Height="30"
                Margin="20"
				Command="{Binding ResetCommand}"
                Content="流程1-1-2" />
			<TextBlock
                Width="80"
                Height="30"
                Margin="20"
                Background="Pink" />
			<TextBox
                Width="80"
                Height="30"
                Margin="20" />
		</StackPanel>
    </Grid>
</UserControl>
